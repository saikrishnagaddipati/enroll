<%if quote.aasm_state == "draft" %>
  <%= form_tag("/broker_agencies/quotes/publish_quote", method: "post") do %>
    <%= hidden_field_tag(:id, quote.id) %>
    <%= hidden_field_tag(:reference_plan_id, plan && plan.id.to_s) %>
    <%= hidden_field_tag(:plan_option_kind, elected_plan_choice) %>
    <%= submit_tag "Publish Quote", class: 'btn btn-primary' %>
  <% end %>
<% end %>
