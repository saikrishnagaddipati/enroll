<div id="qle-details" class='module hidden'>
  <div class="hbx-enrollment-panel panel panel-default module">
    <div class="panel-heading">
      <div class="row">
        <div class="col-xs-11">
          <h4>REPORT LIFE CHANGES</h4>
        </div>
        <div class="col-xs-1">
          <a class="close-popup" href="javascript:void(0);"><span aria-hidden="true" class="glyphicon glyphicon-remove"></span></a>
        </div>
      </div>
      <hr>
    </div>

    <div class="panel-body">
      <div class="row">
        <% url, http_method = '/insured/family_members', :get %>
        <% if action_name == 'find_sep'%>
          <% url, http_method = record_sep_insured_families_path, :post %>
        <% end %>

        <%= form_tag url, method: http_method, class: 'input-no-pd', id: 'qle_form' do %>
          <div class="col-lg-12 col-md-12 col-sm-12 col-xs-12">
            <div id='qle-date-chose'>
              <h3 class="qle-details-title">I got married</h3>

                <h5 class='qle-label'>Enter the date of the event.</h5>
                <div class="form-inline">
                  <div class="form-group">
                    <%= text_field_tag 'qle_date', '', placeholder: "mm/dd/yyyy",
                      class: "required date-picker qle-date-picker form-control", required: true %>
                  </div>
                  <div class="form-group">
                    <%= link_to 'CONTINUE', 'javascript:;', class: 'btn btn-primary btn-md', id: 'qle_submit' %>
                  </div>
                </div>
            </div>


            <div id="qle_reason">
              <div class="moving_reasons hidden">
              
              <br/>
              <h4> Please indicate which of the following circumstances apply to you: </h4> <br/> 
              <div class = 'moving_radio_btn'>
                <div class="n-radio-group">
                    <div class="n-radio-row">
                      <label class="n-radio vertically-aligned-row" for="reason_accept">
                        <%= radio_button_tag :reason, "",  true, id: 'reason_accept', class: "n-radio" %>
                         <span class = "n-radio"></span>
                            <span id="date-change">
                          </span>
                      </label>
                    </div>
                    <div class="n-radio-row">
                      <label class="n-radio" for="reason_accept1">
                        <%= radio_button_tag :reason, "I was living outside the US or in a US territory", false, id: 'reason_accept1', class: "n-radio" %>
                        <span class="n-radio"></span>
                          <span>
                          I was living outside the US or in a US territory
                          </span>
                      </label>
                    </div>
                    <div class="n-radio-row">
                      <label class="n-radio vertically-aligned-row" for="reason_accept2">
                        <%= radio_button_tag :reason, "I had income below 100% of the Federal Poverty Level and was living in a state that had not expanded Medicaid (tool-tip below)", false, id: 'reason_accept2', class: "n-radio" %>
                        <span class="n-radio"></span>
                          <span>
                            I had income below 100% of the Federal Poverty Level and was living in a state that had not expanded Medicaid (tool-tip below) 
                          </span>
                      </label>
                    </div>
                    <div class="n-radio-row">
                      <label class="n-radio" for="reason_reject">
                        <%= radio_button_tag :reason, "None of the Above", false, id:'reason_reject', class: "n-radio" %>
                        <span class="n-radio"></span>
                          <span>
                            None of the Above.
                          </span>
                      </label>
                    </div>
                    <div class = 'calculator'>
                      <br/>
                      Tool-tip: Use this calculator ( <%= link_to('www.healthcare.gov/lower-costs/', 'http://www.healthcare.gov/lower-costs/', target: :_blank) %>) to find out if you meet the qualifications. 
                    </div>
                    <div class="center">
                      <%= link_to 'CONTINUE', 'javascript:;', class: 'btn btn-primary btn-md', id: 'qle_submit_reason' %>
                    </div>
                </div>
              </div>
            </div>
            </div>

            <div id="qle_message">
              <div class="success-info hidden">
                <%= hidden_field_tag 'change_plan' %>
                <%= hidden_field_tag 'qle_id' %>
                <%= hidden_field_tag 'employee_role_id', @employee_role.try(:id) || @employee_role_id || "None" %>

                <p class="success-text">
                  <strong id="qle-input-info">Based on the information you entered, you may be eligible to enroll now but there is limited time.</strong><br>
                </p>
                <div class="effective_on_kinds">
                </div>

                <div class="reasons_for_move">
                </div>

                <div class="center">
                  <%= submit_tag 'Continue', class: 'btn btn-primary' %>
                </div>
              </div>

              <div class="error-info hidden">
                <h4 class="qle-details-title">Change in income that may impact my tax credits/cost-sharing reductions</h4>
                <div class="alert alert-danger" role="alert">
                  <p class="error-text">
                    Based on the information you entered, you may be eligible for a special enrollment period. Please call us at <%= Settings.contact_center.phone_number %> to give us more information so we can see if you qualify.
                  </p>
                </div>
                <br/>
              </div>
            </div>

          </div>
        <% end %>
      </div>
    </div>
  </div>
</div>
