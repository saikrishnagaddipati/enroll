<footer class="footer-wrapper module footer">
  <div class="container">
      <div class="col-md-6">
        <div class="row">
          <%= copyright_notice %>
        </div>
      </div>
      <div class="col-md-6">
        <div class="row">
          <ul class="list-unstyled list-inline pull-right">
            <li>
              <!-- <a href="#"><%= image_tag 'icons/phone.png' %> &nbsp;(855) 532-5465</a> -->
            </li>
            <li>
              <a class="call-customer-service" href="tel:<%= Settings.contact_center.phone_number %>">
