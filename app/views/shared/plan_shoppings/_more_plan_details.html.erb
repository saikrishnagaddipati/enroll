<div class="module shopping-data">
<p class="pull-left">
  <% if @market_kind == 'shop' && @coverage_kind == 'health' %>
    <span class="ttu lg twelve">Employer: </span>
    <strong>
      <%= @benefit_group.try(:plan_year).try(:employer_profile).try(:legal_name) %>
    </strong> &nbsp;&nbsp;
  <% end %>
  <span class="ttu lg twelve">Plans:</span>

<strong id='plans-count'><%= @limited_plans || @plans.count %></strong>
  <br/>
  <div class="pull-left">
    <span class="ttu lg twelve">Coverage For:</span>
    &nbsp;
    <%= plan_shopping_dependent_text(@hbx_enrollment) %>
    &nbsp;&nbsp;
    <% if @market_kind == 'shop' && @coverage_kind == 'health' %>
    Employer: &nbsp;
    <strong><%= @benefit_group.try(:plan_year).try(:employer_profile).try(:legal_name) %></strong>
    &nbsp;&nbsp;
    <% end %>
    <span class="ttu lg twelve">Plans:</span>
    &nbsp;
    <strong id='plans-count'><%= @plans.count %></strong>
  </div>
  <br/>
</div>
