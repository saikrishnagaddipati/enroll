$primary-color: #007bc4;
$primay-active-button-text-color: #ffffff;
$primary-bold: 600;
.module {
	&.shopping-data {
		.dependent-text {
			font-weight: 600;
			text-transform: capitalize;
		}
	&.employer-welcome {
		.left-icon-section {
			margin-bottom: 2.5%;
			a {
				cursor: pointer;
				&:hover {
					text-decoration: underline;
				}
				.fa {
					color: #007bc4;
				}
			}
			&.important {
				margin-top: 7.5%;
				.pdf-image {
					img {
						max-height: 80px;
					}
				}
			}
			h4 {
				margin-top: 0;
			}
			.x-right.dn {
				margin-bottom: 5%;
			}
			.icon-left-download {
				padding: 10px 0;
			}
		}
		.close-right {
			.x {
				img {
					cursor: pointer;
				}
			}
		}
	}
  &.update-browser-warning {
    color: #000;
    position: relative;
    width: 50%;
    top: 15%;
    border: solid 1px #b7b7b7;
    background-color: rgba(256,256,256,1);
    margin: auto;
    text-align: center;
    padding-bottom: 50px;
    .starter {
      margin: auto;
      padding: 15px 20px 20px;
    }
    .bg-grey {
      padding: 10px;
    }
    .browser-icon {
      height: 64px;
      width: 64px;
      margin: 0 auto 7px;
      text-align: center;
      background: url(<%= asset_path("images/broswericons_128.png") %>);
      &.chrome {
        background-position: left top;
      }
      &.safari {
        background-position: right top;
      }
      &.firefox {
        background-position: left bottom;
      }
    }
  }

  &.market-selection {
    padding: 0 15% 5%;
    .btn-primary {
      padding: 4%;
      font-size: auto;
      font-size: 13px;
      background-color: transparent;
      border: solid 1px #ccc;
      border-radius: 3px;
      text-transform: capitalize;
      &.active {
        background-color: $primary-color;
        a {
          color: $primay-active-button-text-color;
        }
      }
    }
  }
  &.ivl-documents-table {
    margin-top: 1.25%;
    .demo-up {
      border: 0 !important;
      -webkit-appearance: none;
      -moz-appearance: none;
      width: 150px;
      text-overflow: "";
      border-radius: 15px;
      padding: 0px;
      margin: 0px;
    }

    &.verif-type {
      margin-right: 20px;
    }

  	&.modal-footer {
  		.modal-sbc-link {
  			text-align: left;
  			position: relative;
  			.fa.fa-file-pdf-o {
  				margin: 2px auto auto 5px;
  			}
  			.sbc-text {
  				font-size: 14px;
  				margin-left: 10px;
  				position: absolute;
  				white-space: nowrap;
  				top: 10px;
  			}
  		}
  	}
    .table {
      width: 100%;
      .thead-default {
        tr {
          background-color: #fff;
          th {
            text-align: center;
            vertical-align: middle;
          }
        }
      }
      tbody {
        tr {
          &.top {
            &.even {
              background-color: #ffffff !important;
              &:hover {
                background-color: #f5f5f5 !important;
              }
            }
          }
          &.dn {
            &:hover {
              background-color: transparent;
            }
            td.td-container {
              padding: 0px 12px;
              .table-responsive {
                overflow: visible;
              }
            }
            table {
              &.table {
                background-color: #fff;
                margin: 0 0 12px;
                tr {
                  &.bt-n {
                    td {
                      border-top: none !important;
                    }
                  }
                  &.bb {
                    td {
                      border-bottom: 1px solid #ddd;
                    }
                  }
                  td {
                    padding: 10px 12px;
                    .fa {
                      &.fa-download {
                        color: #b7b7b7;
                        &:hover {
                          color: #23527c;
                        }
                      }
                      &.fa-comments-o {
                        color: #b7b7b7;
                        margin-left: 20px;
                        &:hover {
                          color: #23527c;
                        }
                      }
                    }
                  }
                }
                .upload {

                  td.td-container {
                    padding: 6px 12px 30px;
                  }
                }
              }
            }
          }
          td {
            vertical-align: middle;
						.sub-table {
							width: 100%;
							table-layout: auto;
							td {
								padding: 1.25% 1.25%;
							}
							.select-upload {
								float: left;
							}
							.doc-upload-file {
							  font-size: 10px;
								padding: 0;
								width:170px;
							}
							.selectric {
								border-radius: 5px;
								max-width: 140px;
								width: 140px;
							}
							.btn-trans {
								background-color: transparent;
							}
						}
            &.table-functions {
							a {
								&:hover {
									color: #23527c !important;
								}
								span {
									text-align: center !important;
									float: none !important;
									white-space: nowrap;

									.function-text {
										font-size: 12px;
										color: #b7b7b7;
									}
								}
							}
              .fa {
                cursor: pointer;
                display: inline-block;
                color: #b7b7b7;
              }
            }
          }
          &:hover {
            td {
              &.table-functions {
                .fa {
                  &:hover {
                    color: #23527c;
                  }
                }
              }
            }
          }
        }
      }
    }
  }
  &.my-account-widget {
    &.panel {
      border-radius: 0;
      background-image: none;
      border-bottom: none;
      padding: 0 20px;
      font-size: 14px;
      text-transform: uppercase;
      color: #ffffff;
      margin: 0;
      padding: 0 15px;
      .panel-heading {
        background-color: #ffffff;
        background-image: none;
        border-bottom: none;
        padding: 0;
        font-size: 12px;
        color: #333333;
        text-transform: capitalize;
        text-align: center;
        h4 {
          font-weight: 600;
          font-size: 14px;
          margin-bottom: 0;
          max-width: 155px;
          a {

          }
        }
      }
      .panel-body {
        color: #9b9b9b;
        text-transform: uppercase;
        font-size: 14px;
        text-align: center;
        .fte-count {
          color: #007bc4;
        }
      }
    }
  }
	&.census-employees-table {
		margin-top: 1.25%;
		.table {
			.thead-default {
				tr {
					background-color: #fff;
					th {
						text-align: center;
						vertical-align: middle;
					}
				}
			}
			tbody {
				tr {
					&.top {
						&.even {
							background-color: #fff;
							&:hover {
								background-color: #f5f5f5;
							}
						}
					}
					td {
						vertical-align: middle;
						&.table-functions {
							min-width: 70px;
							.fa {
								cursor: pointer;
								display: inline-block;
								color: #b7b7b7;
								margin-left: 5px;
							}
						}
					}
					&:hover {
						td {
							&.table-functions {
								.fa {
									&:hover {
										color: #23527c;
									}
								}
							}
						}
					}
				}
			}
		}
	}
	&.health_analytics {
		.doctors {
			.my-doctors {
				.my-doctor {
					border: solid 1px $primary-color;
					border-radius: 4px;
					font-size: 12px;
					padding: 0.5%;
					margin-right: 1%;
					margin-bottom: 1%;
					display: inline-block;
					span {
						padding-left: 3px;
						white-space: nowrap;
						display: inline-block;
						.fa {
							margin: 0 3px;
							cursor: pointer;
						}
					}
				}
			}
		}
		.typeahead {
			background-color: #fff !important;
		}
		.overall-health {

			.panel-body {
				padding: 25px 15px 0;
			}
		}
		.medications, .conditions {
			.panel-heading {
				background: none;
			}
		}
		&.visits-estimate {
			.btn-trans {
				margin: auto;
				display: block;
				max-width: 200px;
			}
		}
		.healthConditions, .healthMedications {
			span {
				font-size: 20px !important;
				top: -4px !important;
			}
			.checkbox {
				margin-bottom: 4px !important;
			}
		}
		#continue {
			width: 100%;
		}
		.selectric-wrapper {
			top: -10px;
		}
		.family-member {
			.slider {
				margin-left: 5%;
				width: 90%;
				.slider-selection {
					background-image: none;
					filter: none;
					box-shadow: none;
					&.tick-slider-selection {
						background-image: none;
						filter: none;
					}
				}
				.slider-tick.round {
					display: none;
				}
				.slider-tick-label {
					font-size: 12px;
				}
			}
			.first-name, .middle-name, .last-name, .gender {
				text-transform: capitalize;
			}
			h5 {
				text-align: center;
			}
			span {
				color: #007bc4;
			}
			.panel-heading {
				border-bottom: solid 1px #eee;
			}
			.panel-footer {
				background-color: #d9edf7;
				border-color: #bce8f1
				span {
						color: #31708f;
					}
			}
		}
		input[type=checkbox] {
       display: none;
       + label > i.fa {
         color: #69A30D;
       }
     }
     .checkbox {
       display: block;
       margin: auto;
     }
     .checkbox {
       &>div {
         display: inline-block;
         margin: auto;
       }
     }
     .checkbox label {
       padding-left: 0;
     }
     .checkbox label label {
       display: inline;
       padding-left: 0;
     }
     .checkbox label label + span {
       display: inline-block;
       position: relative;
       text-transform: capitalize;
       font-size: 12px;
       top: -6px;
       left: 5px;
     }
     input[type=checkbox]:disabled + label i {
       color: #b7b7b7;
       cursor: not-allowed;
     }
     input[type=checkbox] + label i {
       &:first-of-type {
         display: none;

       }
       &:last-of-type {
         display: inline-block;
         width: 30px;
       }
     }
     input[type=checkbox]:checked + label i {
       &:first-of-type {
         display: inline-block;
         width: 30px;
       }
       &:last-of-type {
         display: none;
       }
     }
		 &.wellness-plans {
			 padding: 15px 0;
		 }
	}
}
