$primary-blue: #007bc4;
$primary-green: #69A30D;
$primay-active-button-text-color: #ffffff;
$primary-bold: 600;
$primary-white: #fff;
$primary-black: #5C5C5C;

.module {
	&.shopping-data {
		.dependent-text {
			font-weight: 600;
			text-transform: capitalize;
		}
	}
	&.employer-welcome {
		.left-icon-section {
			margin-bottom: 2.5%;
			a {
				cursor: pointer;
				&:hover {
					text-decoration: underline;
				}
				.fa {
					color: #007bc4;
				}
			}
			&.important {
				margin-top: 7.5%;
				.pdf-image {
					img {
						max-height: 80px;
					}
				}
			}
			h4 {
				margin-top: 0;
			}
			.x-right.dn {
				margin-bottom: 5%;
			}
			.icon-left-download {
				padding: 10px 0;
			}
		}
		.close-right {
			.x {
				img {
					cursor: pointer;
				}
			}
		}
	}
  &.footer {
    font-size: 14px;
    color: $primary-white;
    background-color: #003260;
    padding: 15px;
    position: absolute;
    bottom: 0;
    left: 0;
    width: 100%;
    a {
      color: $primary-white;
    }
    &>.container {
      display: table;
      .col-md-6 {
        display: table-cell;
        float: none;
        vertical-align: middle;
      }
      .col-md-6:last-of-type {
        a, span {
          display: inline-block;
        }
        ul {
          margin: 0;
        }
      }
    }
  }
  &.welcome {
    h1 {
      color: $primary-black;
    }
		.btn {
			margin: 3px initial;
		}
  }
  &.signin {
    .actions {
      span {
        margin-top: 2.5%;
      }
    }
  }
  &.update-browser-warning {
    color: #000;
    position: relative;
    width: 50%;
    top: 15%;
    border: solid 1px #b7b7b7;
    background-color: rgba(256,256,256,1);
    margin: auto;
    text-align: center;
    padding-bottom: 50px;
    .starter {
      margin: auto;
      padding: 15px 20px 20px;
    }
    .bg-grey {
      padding: 10px;
    }
    .browser-icon {
      height: 64px;
      width: 64px;
      margin: 0 auto 7px;
      text-align: center;
      background: url('<%= asset_path("images/broswericons_128.png") %>');
      &.chrome {
        background-position: left top;
      }
      &.safari {
        background-position: right top;
      }
      &.firefox {
        background-position: left bottom;
      }
    }
  }

  &.market-selection {
    padding: 0 15% 5%;
    .btn-primary {
      padding: 4%;
      font-size: auto;
      font-size: 13px;
      background-color: transparent;
      border: solid 1px #ccc;
      border-radius: 3px;
      text-transform: capitalize;
      &.active {
        background-color: $primary-blue;
        a {
          color: $primay-active-button-text-color;
        }
      }
    }
  }
  &.modal-footer {
    .modal-sbc-link {
      text-align: left;
      position: relative;
      .fa.fa-file-pdf-o {
        margin: 2px auto auto 5px;
      }
      .sbc-text {
        font-size: 14px;
        margin-left: 10px;
        position: absolute;
        white-space: nowrap;
        top: 10px;
      }
    }
  }
  &.my-account-widget {
    &.panel {
      border-radius: 0;
      background-image: none;
      border-bottom: none;
      padding: 0 20px;
      font-size: 14px;
      text-transform: uppercase;
      color: #ffffff;
      margin: 0;
      padding: 0 15px;
      .panel-heading {
        background-color: #ffffff;
        background-image: none;
        border-bottom: none;
        padding: 0;
        font-size: 12px;
        color: #333333;
        text-transform: capitalize;
        text-align: center;
        h4 {
          font-weight: 600;
          font-size: 14px;
          margin-bottom: 0;
          max-width: 155px;
          a {

          }
        }
      }
      .panel-body {
        color: #9b9b9b;
        text-transform: uppercase;
        font-size: 14px;
        text-align: center;
        .fte-count {
          color: #007bc4;
        }
      }
    }
  }
	&.census-employees-table {
		margin-top: 1.25%;
		.table {
			.thead-default {
				tr {
					background-color: #fff;
					th {
						text-align: center;
						vertical-align: middle;
					}
				}
			}
			tbody {
				tr {
					&.top {
						&.even {
							background-color: #fff;
							&:hover {
								background-color: #f5f5f5;
							}
						}
					}
					td {
						vertical-align: middle;
						&.table-functions {
							min-width: 70px;
							.fa {
								cursor: pointer;
								display: inline-block;
								color: #b7b7b7;
								margin-left: 5px;
							}
						}
					}
					&:hover {
						td {
							&.table-functions {
								.fa {
									&:hover {
										color: #23527c;
									}
								}
							}
						}
					}
				}
			}
		}
	}
	&.health_analytics {
		.doctors {
			.my-doctors {
				.my-doctor {
					border: solid 1px $primary-blue;
					border-radius: 4px;
					font-size: 12px;
					padding: 0.5%;
					margin-right: 1%;
					margin-bottom: 1%;
					display: inline-block;
					span {
						padding-left: 3px;
						white-space: nowrap;
						display: inline-block;
						.fa {
							margin: 0 3px;
							cursor: pointer;
						}
					}
				}
			}
		}
		.typeahead {
			background-color: #fff !important;
		}
		.overall-health {

			.panel-body {
				padding: 25px 15px 0;
			}
		}
		.medications, .conditions {
			.panel-heading {
				background: none;
			}
		}
		&.visits-estimate {
			.btn-default {
				margin: auto;
				display: block;
				max-width: 200px;
			}
		}
		.healthConditions, .healthMedications {
			span {
				font-size: 20px !important;
				top: -4px !important;
			}
			.checkbox {
				margin-bottom: 4px !important;
			}
		}
		#continue {
			width: 100%;
		}
		.selectric-wrapper {
			top: -10px;
		}
		.family-member {
			.slider {
				margin-left: 5%;
				width: 90%;
				.slider-selection {
					background-image: none;
					filter: none;
					box-shadow: none;
					&.tick-slider-selection {
						background-image: none;
						filter: none;
					}
				}
				.slider-tick.round {
					display: none;
				}
				.slider-tick-label {
					font-size: 12px;
				}
			}
			.first-name, .middle-name, .last-name, .gender {
				text-transform: capitalize;
			}
			h5 {
				text-align: center;
			}
			span {
				color: #007bc4;
			}
			.panel-heading {
				border-bottom: solid 1px #eee;
			}
			.panel-footer {
				background-color: #d9edf7;
				border-color: #bce8f1
				span {
						color: #31708f;
					}
			}
		}
		input[type=checkbox] {
       display: none;
       + label > i.fa {
         color: #69A30D;
       }
     }
     .checkbox {
       display: block;
       margin: auto;
     }
     .checkbox {
       &>div {
         display: inline-block;
         margin: auto;
       }
     }
     .checkbox label {
       padding-left: 0;
     }
     .checkbox label label {
       display: inline;
       padding-left: 0;
     }
     .checkbox label label + span {
       display: inline-block;
       position: relative;
       text-transform: capitalize;
       font-size: 12px;
       top: -6px;
       left: 5px;
     }
     input[type=checkbox]:disabled + label i {
       color: #b7b7b7;
       cursor: not-allowed;
     }
     input[type=checkbox] + label i {
       &:first-of-type {
         display: none;

       }
       &:last-of-type {
         display: inline-block;
         width: 30px;
       }
     }
     input[type=checkbox]:checked + label i {
       &:first-of-type {
         display: inline-block;
         width: 30px;
       }
       &:last-of-type {
         display: none;
       }
     }
		 &.wellness-plans {
			 padding: 15px 0;
		 }
	}
}
