$primary-color: #007bc4;

.compare-table {
  .key {
    margin-bottom: 8px;
    >.col-xs-3 {
      width:33.75%;
      &:first-of-type {
        width: 20%;
      }
      &:last-of-type {
        width: 20%;
      }
      .tooltip {
        .tooltip-inner {
          max-width: 300px;
        }
      }
    }
    .ball {
      width: 15px;
      height: 15px;
      border-radius: 8px;
      display: inline-block;
      background-color: #eeac57;
      margin-right: 7px;
      top: 3px;
      position: relative;
      &.green {
        background-color: #5fb760;
      }
      &.red {
        background-color: #d75452;
        position: relative;
        vertical-align: baseline;
      }
    }
    label {
      display: inline;
      font-size:11px;
    }

  }
  .graph {
    width:100px;
    margin: auto;
    .progress-bar-vertical {
      width: 50px;
      min-height: 140px;
      display: flex;
      align-items: flex-end;
      float: left;
      position: relative;
      overflow: visible;
      .tooltip {
        width: 200px !important;
      }
    }
    .progress {
      margin-bottom: 0;
    }
    .progress-bar {
      position:absolute;
      bottom: 0;
    }

    .progress-bar-vertical .progress-bar {
      width: 100%;
      height: 0;
      -webkit-transition: height 0.6s ease;
      -o-transition: height 0.6s ease;
      transition: height 0.6s ease;
    }

    .progress:first-of-type {
      border-top-right-radius: 0;
      border-bottom-right-radius: 0;
      .progress-bar {
        border-bottom-left-radius: 4px;
      }
    }
    .progress:last-of-type {
      border-top-left-radius: 0;
      border-bottom-left-radius: 0;
      .progress-bar {
        border-bottom-right-radius: 4px;
      }
    }
    .progress-bar-striped {
      background-size: auto;
    }
  }
}
