.module {
  &#qle-details {
    .error-info {
      margin-top: 0;
    }
  }
}
.brokers-widget {
  .col-xs-8 {
    padding-top: 10px;
    h4 {
      padding: 15px 0 0 0;
      margin: 0;
      font-weight: 600;
    }
  }

  h4, .fa {
    color: #007bc4;
  }

  .col-xs-4 {
    cursor: pointer;
    &:hover {
      * {
        color: #23527c;
      }
    }
    h2 {
      margin-bottom: 0;
      margin-top: 25px;
      img {
        width: 85%;
      }
    }
    span {
      color: #007bc4;
    }
  }
}
.help-signup {
  h4 {
    color: #007bc4;
    margin-bottom: 0;
    font-weight: 600;
    &+small {
      display: inline-block;
      padding: 3px 0 10px;
    }
  }
}
#profile-content #address_info .mailing-div {
 display: block;
 &>.col-lg-3>* {
   display: none;
 }
}
.carrier-contact {
  padding-bottom: 20px;
  h4.modal-title {
    text-align: center;
  }
  i {
    text-align: center;
    color: #007bc4;
    display: block;
  }
  .fa-phone {
    font-size: 42px;
    padding-bottom: 10px;
  }
  .carrier-numbers {
    text-align: center;

  }
}

.fa-icon-link {
  padding-top: 4.45%;
}
.fa-icon-link, .fa-icon-label, a {
  color: #007bc4;
  cursor: pointer;
  &:hover {
    color: #23527c;
  }
}
.fa-icon-label {
  cursor: pointer;
  color: #595959;
  font-size: 12px;
}

  .benefits-tab, .home-tab, .hbx-enrollment-panel {
    .fa {
      color: #007bc4;
      margin: 0 10px 0 5px;
      &.published, &.enrolling {
        color: #FFD700;
      }
    }
    .bg-title {
      margin-bottom: 10px;
      display: inline-block;
      color: #69A30D;
      font-size: 18px;
    }
  }
  .hbx-enrollment-panel {
    table {
      tr {
        td {
          vertical-align: top;
          .enrollment-created-at {
            white-space: nowrap;
          }
        }
      }
    }
    .alert {
      padding: 15px 40px 15px 15px;
    }
    .btn-primary {
      text-transform: uppercase;
      padding: 10px 20px;
      border-radius: 6px;
      font-size: 16px;
      letter-spacing: 1px;
      border-top: none;
      border-left: none;
      border-right: none;
      margin-top: 5px;
      &.mtz {
        margin-top: 0;
      }
    }
    .btn.pull-right {
      width: 100%;
      max-width: 75%;
    }
        .bg-title {
          font-weight: bold;
        }
        .panel-body {
          .col-xs-12 {
            padding-top: 15px;
            &.btn-stack {
              padding-top: 0;
              a {
                margin-top: 0;
              }
            }
            &.btn-stack:last-of-type {
              padding-top: 5px;
            }
          }
        }
        .panel-body {
          h4 {
            margin-top: 5px;
            font-weight: 400;
            font-size: 18px;
          }
          .info p {
            margin: 0;
          }
        }
  }

.compare-table {
  tr {
    td:first-of-type, th:first-of-type {
      background-color: #E8E8E8;
      border: none;
      text-align: left;
      vertical-align: center;
      font-size: 12px;
      &.row-title {
        font-size: 24px;
        color: #414141;
        font-weight: 600;
      }
    }
    th, td {
      text-align: center;
      vertical-align: center;
      img {
      }
    }
    td, th {
      border-left: 1px solid #ddd;
      border-right: none;
      &:first-of-type {

      }
    }

  }
  .row-title {
    font-size: 24px;
    color: #414141;
    font-weight: 600;
  }
}
.module {
  &.plan-row {
    .sbc_link {
      margin-top: 6px;
    }
  }
  .sbc_link {
    display: inline-block;
    &.dental {
      position: relative;
      span {
        position: absolute;
        margin-top: 10px !important;
        white-space: nowrap;
      }
    }
  }
}
.modal {
  
}
.horizontal-status {
  padding: 3.75% 0 1.25%;
  .step {

  }
  .arrow {
    padding-top: 1.25%;
    img {
      width: 100%;
      max-width: 100%;
    }
  }
}
